    "commandnotfound"
  ],
  "author": "lengthmin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lengthmin/command-not-found/issues"
  },
  "homepage": "https://github.com/lengthmin/command-not-found#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.5",
    "got": "^11.8.2",
    "ora": "^5.4.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.28",
    "@types/node": "^14.14.37",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "publishConfig": {
